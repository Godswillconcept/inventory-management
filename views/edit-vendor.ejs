<%- include('./includes/header') %>
  <div class="container mt-5">
    <h1 class="my-3 text-dark">Update Vendor</h1>
    <form action="" method="post" class="row my-5" enctype="multipart/form-data">
      <div class="form-group col-md-12 col-lg-6 mb-3">
        <label>Name</label>
        <input type="text" name="name" value="<%= vendor.name %> " class="form-control" />
      </div>
      <div class="form-group col-md-12 col-lg-6 mb-3">
        <label>Email</label>
        <input type="email" name="email" value="<%= vendor.email %> " class="form-control" />
      </div>
      <div class="form-group col-md-12 col-lg-6 mb-3">
        <label>Phone Number</label>
        <input type="tel" name="phone" value="<%= vendor.phone %> " class="form-control" />
      </div>

      <div class="form-group col-md-12 col-lg-6 mb-3">
        <label>Address</label>
        <textarea class="form-control" id="address" name="address" rows="2">
<%= vendor.address %></textarea>
      </div>
      <div class="form-group col-md-12 col-lg-6 mb-3">
        <label>Photo</label>
        <input type="file" name="photo" value="<%= vendor.photo %> " class="form-control" />
      </div>
      <button type="submit" class="btn btn-primary col-12">Submit</button>
    </form>
  </div>
  <%- include('./includes/footer') %>